{
    "name": "ideaa",
    "version": "1.0.0",
    "description": "One page website template",
    "keywords": [
        "html",
        "css",
        "javascript",
        "pug",
        "sass"
    ],
    "homepage": "https://i7zaari.github.io/ideaa",
    "bugs": "https://github.com/i7zaari/ideaa/issues",
    "license": "CC-BY-NC-SA-4.0",
    "author": {
        "name": "Hamza Zaari",
        "email": "i7zaari@yandex.com",
        "url": "https://i7zaari.github.io/"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/i7zaari/ideaa.git"
    },
    "source": [
        "./src/pug/pages/index.pug",
        "./src/pug/pages/404.pug"
    ],
    "parcel-namer-struct": [{
            "type": [
                "jpg",
                "png",
                "svg"
            ],
            "dest": "images"
        },
        {
            "type": [
                "woff2",
                "woff",
                "ttf"
            ],
            "dest": "fonts"
        },
        {
            "type": [
                "css"
            ],
            "dest": "css"
        },
        {
            "type": [
                "js"
            ],
            "dest": "javascript"
        }
    ],
    "scripts": {
        "dev": "./scripts/dev.sh",
        "build": "./scripts/build.sh"
    },
    "devDependencies": {
        "@parcel/resolver-glob": "^2.4.1",
        "@parcel/transformer-pug": "^2.4.1",
        "@parcel/transformer-sass": "^2.4.1",
        "parcel": "^2.4.1",
        "parcel-namer-struct": "^1.0.0"
    }
}